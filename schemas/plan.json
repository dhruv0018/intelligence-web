{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Plan Schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "startDate": {
            "type": "string",
            "format": "date-time"
        },
        "endDate": {
            "type": "string",
            "format": "date-time"
        },
        "maxGamesPerPlan": {
            "type": "integer"
        },
        "maxAnyGames": {
            "type": "integer"
        },
        "maxTurnaroundTime": {
            "type": "integer"
        },
        "price": {
            "type": "integer"
        }
    },
    "additionalProperties": false
}
